{
  "name": "vobar/laravel-basic-components",
  "description": "Laravel package basic components for small sites",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Vladimir vobar Barkaev"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0",
    "illuminate/console": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.2",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Vobar\\LaravelBasicComponents\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vobar\\LaravelBasicComponents\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Vobar\\LaravelBasicComponents\\BasicComponentsServiceProvider"
      ]
    }
  }
}
